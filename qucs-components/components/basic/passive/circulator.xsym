<component name="Circulator" version="0.1.0" label="Circu">
   <displaytext>
      <lang lang="C">Ideal circulateur</lang>
      <lang lang="de">Zirkulator</lang>
      <lang lang="es">Circulador</lang>
      <lang lang="fr">Circulateur idéal</lang>
      <lang lang="he">מחוגג</lang>
