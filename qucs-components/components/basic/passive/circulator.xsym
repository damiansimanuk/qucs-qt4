   <description>
      <lang lang="C">A device where output port depend on the input port.</lang>
      <lang lang="fr">Un composant dans lequel le port de sortie depend du port d'entrée.</lang>
   </description>

   <schematics default="ISOlike">
     <schematic name="ISOlike" href="svg/circulator-isolike.svg">
       <port name="1" x="-40" y="-30"/>
       <port name="2" x="40" y="-30"/>
       <port name="3" x="0" y="40"/>
     </schematic>
     <schematic name="qucs0" href="svg/circulator-qucs0.svg">
       <port name="1" x="-30" y="0"/>
       <port name="2" x="30" y="0"/>
       <port name="3" x="0" y="30"/>
     </schematic>
   </schematics>

   <ports>
      <port name="1" type="analog"/>
      <port name="2" type="analog"/>
      <port name="3" type="analog"/>
   </ports>

